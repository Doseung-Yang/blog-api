#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangdoseung/Documents/Hi_Doseung/doseung-blog-api/blog-project/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.10_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.26.10___ruvj6zpo4g5fk7i5eolpazbs54/node_modules/ts-jest/node_modules:/Users/yangdoseung/Documents/Hi_Doseung/doseung-blog-api/blog-project/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.10_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.26.10___ruvj6zpo4g5fk7i5eolpazbs54/node_modules:/Users/yangdoseung/Documents/Hi_Doseung/doseung-blog-api/blog-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangdoseung/Documents/Hi_Doseung/doseung-blog-api/blog-project/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.10_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.26.10___ruvj6zpo4g5fk7i5eolpazbs54/node_modules/ts-jest/node_modules:/Users/yangdoseung/Documents/Hi_Doseung/doseung-blog-api/blog-project/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.26.10_@types+jest@27.5.2_babel-jest@27.5.1_@babel+core@7.26.10___ruvj6zpo4g5fk7i5eolpazbs54/node_modules:/Users/yangdoseung/Documents/Hi_Doseung/doseung-blog-api/blog-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
